{
    "manifest_version": 2,
    "name": "Social Site Blocker",
    "version": "0.1.1",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["Content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "Popup.html",
      "default_title": "Social Site Blocker"
    },
    "permissions": [
      "https://www.youtube.com/*",
      "https://www.facebook.com/*",
      "https://www.netflix.com/*"
    ]
  }